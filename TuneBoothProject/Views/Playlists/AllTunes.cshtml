@model TuneBooth.Models.ModelsVM

@{
    ViewBag.Title = "AllTunes";
}

<h2>Mes musiques</h2>

<table class="table">
    <tr>
        <th>
            Titre
        </th>
        <th></th>
    </tr>

    @foreach (var tune in Model.Tunes)
    {
        <tr>
            <td>
                <a href="~/Tunes/Details/@tune.ID">@tune.Titre</a>
            </td>
        </tr>
    }

